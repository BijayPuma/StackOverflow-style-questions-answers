{{#each questions}}
<div class="card card-body">
    <h4>{{title}}</h4>
    <p>{{question}}</p>
    <h6>{{date}}</h6>
    <a class='btn btn-dark btn-block mb-2' href='/questions/edit/{{id}}'>Edit</a>
    <a class='btn btn-dark btn-block mb-2' href='/questions/delete/{{id}}'>Delete</a>


</div> {{else}} <p>No questions posted</p>

{{/each}}